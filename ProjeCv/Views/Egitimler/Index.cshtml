@model ProjeCv.Models.Class.Class1
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                ID
            </th>
            <th>
                BAŞLIK
            </th>
            <th>
                ALT BAŞLIK
            </th>
            <th>
                BÖLÜM
            </th>
            <th>
                DNO
            </th>
    </thead>
    <tbody>
        @foreach (var item in Model.Deger3)
        {
            <tr>
                <th>@item.Id</th>
                <td>@item.Title</td>
                <td>@item.SubTitle</td>
                <td>@item.Department</td>
                <td>@item.Gpa</td>
            </tr>
        }

    </tbody>
</table>

<a href="/Egitimler/YeniEgitim" class="btn btn-info">Yeni Eğitim Ekle</a>